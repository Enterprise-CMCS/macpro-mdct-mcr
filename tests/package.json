  "main": "index.js",
  "scripts": {
    "start": "cd ../ && ./run local && cd -",
    "test": "concurrently --kill-others \"yarn start\" \"yarn test:e2e-ui\"",
    "test:e2e": "playwright test",
    "test:e2e-ui": "playwright test --ui",
    "test:e2e-stable": "playwright test --grep-invert \"@flaky|@regression|@a11y\"",
    "test:e2e-stable-ui": "playwright test --ui --grep-invert \"@flaky|@regression|@a11y\""
  },
  "dependencies": {
    "ws": "^7.5.10"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@faker-js/faker": "^10.3.0",
