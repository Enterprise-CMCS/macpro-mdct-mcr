{
  "name": "ui-src",
  "description": "",
  "version": "1.0.0",
  "license": "CC0-1.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "coverage": "jest --coverage",
    "test": "jest --setupFiles dotenv/config",
    "watchTest": "jest --setupFiles dotenv/config --watch",
    "a11y": "pa11y --runner axe --runner htmlcs",
    "eject": "react-scripts eject",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.0",
    "@cmsgov/design-system": "^3.4.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@hookform/resolvers": "^2.8.4",
    "aws-amplify": "^4.3.4",
    "bootstrap": "^5.1.3",
    "date-fns": "^2.26.0",
    "date-fns-tz": "^1.2.2",
    "framer-motion": "^4",
    "object-path": "^0.11.8",
    "react": "^17.0.1",
    "react-bootstrap": "^2.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.4.2",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.20.5",
    "react-icons": "^4.3.1",
    "react-query": "^3.34.5",
    "react-router-dom": "6.0.2",
    "react-scripts": "^5.0.0",
    "sass": "^1.37.5",
    "schema-to-yup": "^1.11.12",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.38.0",
    "@babel/core": "^7.13.16",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.16.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^27.0.2",
    "@types/jest-axe": "^3.5.3",
    "@types/node": "^16.11.6",
    "@types/object-path": "^0.11.1",
    "@types/react": "^17.0.33",
    "@types/react-bootstrap": "^0.32.28",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.2",
    "@types/uuid": "^8.3.3",
    "axe-core": "^4.4.1",
    "babel": "^6.23.0",
    "babel-jest": "^26.6.3",
    "jest-axe": "^6.0.0",
    "pa11y": "^6.2.3",
    "pa11y-runner-htmlcs": "^2.0.1",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "serverless-cloudfront-invalidate": "^1.5.0",
    "serverless-s3-sync": "^1.14.4",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "@types/react": "17.0.45",
    "@types/react-dom": "17.0.17"
  },
  "jest": {
    "coverageReporters": [
      "json",
      [
        "lcov",
        {
          "projectRoot": "../../"
        }
      ],
      "text",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{ts,tsx}",
      "!src/components/index.tsx",
      "src/utils/**/*.{ts,tsx}",
      "!src/utils/**/index.ts",
      "!src/utils/constants/*",
      "!src/utils/testing/*",
      "!src/utils/types/*",
      "src/views/**/*.{ts,tsx}",
      "!src/views/index.tsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|webp|svg|mp4|webm|wav|mp3|m4a)$": "<rootDir>/src/utils/testing/mockAsset.ts"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/utils/testing/setupJest.tsx"
    ],
    "globals": {
      "_env_": {}
    },
    "testEnvironment": "jest-environment-jsdom"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
