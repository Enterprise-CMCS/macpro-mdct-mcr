{
  "id": "addAdminBanner",
  "options": {
    "mode": "onChange"
  },
  "fields": [
    {
      "id": "bannerTitle",
      "type": "text",
      "validation": "text",
      "props": {
        "label": "Title text",
        "placeholder": "New banner title"
      }
    },
    {
      "id": "bannerDescription",
      "type": "textarea",
      "validation": "text",
      "props": {
        "label": "Description text",
        "placeholder": "New banner description"
      }
    },
    {
      "id": "bannerLink",
      "type": "text",
      "validation": "urlOptional",
      "props": {
        "label": "Link",
        "requirementLabel": "Optional"
      }
    },
    {
      "id": "bannerStartDate",
      "type": "date",
      "validation": "date",
      "props": {
        "label": "Start date",
        "timetype": "startDate"
      }
    },
    {
      "id": "bannerEndDate",
      "type": "date",
      "validation": {
        "type": "endDate",
        "dependentFieldName": "bannerStartDate"
      },
      "props": {
        "label": "End date",
        "timetype": "endDate"
      }
    }
  ]
}
