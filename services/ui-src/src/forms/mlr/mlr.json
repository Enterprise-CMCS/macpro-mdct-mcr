            "id": "mlrSection3Header",
            "type": "sectionHeader",
            "props": {
              "divider": "top",
              "content": "3. MLR Calculation"
            }
          },
          {
            "id": "report_requiredMemberMonths",
            "type": "number",
            "validation": "number",
            "props": {
              "label": "3.1 Member months",
              "hint": [
                {
                  "type": "p",
                  "content": "Enter the required number of member months."
                },
                {
                  "type": "externalLink",
                  "content": "42 CFR § 438.8(b)",
                  "props": {
                    "href": "https://www.ecfr.gov/current/title-42/chapter-IV/subchapter-C/part-438/subpart-A/section-438.8#p-438.8(b)",
                    "target": "_blank",
                    "aria-label": "Link opens in new tab"
                  }
                }
              ]
            }
          },
          {
            "id": "report_unadjustedMlrPercentage",
            "type": "number",
            "validation": "numberOptional",
            "props": {
              "label": "3.2 Unadjusted MLR",
              "hint": "Enter the percentage exactly as the percentage should appear (rounded to nearest tenth). Entering '1' will result in 1% instead of 100%. (optional subcomponent of section 3.4 Adjusted MLR)",
              "mask": "percentage",
              "styleAsOptional": true
            }
          },
          {
            "id": "report_unadjustedMlrPercentageExplanation",
            "type": "textarea",
            "validation": "textOptional",
            "props": {
              "label": "Unadjusted MLR explanation",
              "hint": "If the optional <i>Unadjusted MLR</i> reported in section 3.2 does not equal the ratio of the <i>MLR numerator</i> over the <i>MLR denominator</i> (as reported in sections 1.3 and 2.3), states may provide an explanation here.",
              "styleAsOptional": true
            }
          },
          {
            "id": "report_credibilityAdjustmentPercentage",
            "type": "number",
            "validation": "numberOptional",
            "props": {
              "label": "3.3 Credibility adjustment",
              "hint": [
                {
                  "type": "p",
                  "content": "Enter the percentage exactly as the percentage should appear (rounded to nearest tenth). (Optional subcomponent of section 3.4 Adjusted MLR)"
                },
                {
                  "type": "ul",
                  "content": "",
                  "children": [
                    {
                      "type": "li",
                      "children": [
                        {
                          "type": "span",
                          "content": "Enter 0% if no credibility adjustment is needed."
                        }
                      ]
                    },
                    {
                      "type": "li",
                      "children": [
                        {
                          "type": "span",
                          "content": "For non-credible plans, the credibility adjustment should be rounded to the nearest tenth and entered up to 100%."
                        }
                      ]
                    },
                    {
                      "type": "li",
                      "children": [
                        {
                          "type": "span",
                          "content": "For fully credible plans, the credibility adjustment should be entered as 0%."
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "externalLink",
