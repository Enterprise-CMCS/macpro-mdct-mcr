            "id": "mlrSection2Header",
            "type": "sectionHeader",
            "props": {
              "divider": "top",
              "content": "2. Medical Loss Ratio Denominator"
            }
          },
          {
            "id": "report_premiumRevenue",
            "type": "number",
            "validation": "numberOptional",
            "props": {
              "label": "2.1 Premium revenue",
              "hint": [
                {
                  "type": "p",
                  "content": "Enter premium revenue (optional subcomponent of section 2.3 MLR denominator)."
                },
                {
                  "type": "externalLink",
                  "content": "42 CFR § 438.8(f)(2)",
                  "props": {
                    "href": "https://www.ecfr.gov/current/title-42/chapter-IV/subchapter-C/part-438/subpart-A/section-438.8#p-438.8(f)(2)",
                    "target": "_blank",
                    "aria-label": "Link opens in new tab"
                  }
                }
              ],
              "mask": "currency",
              "styleAsOptional": true
            }
          },
          {
            "id": "report_taxesLicensingRegulatoryFees",
            "type": "number",
            "validation": "numberOptional",
            "props": {
              "label": "2.2 Federal, State, and local taxes and licensing and regulatory fees",
              "hint": [
                {
                  "type": "p",
                  "content": "Enter Federal, State, and local taxes and licensing; and regulatory fees (optional subcomponent of section 2.3 MLR denominator)."
                },
                {
                  "type": "externalLink",
                  "content": "42 CFR § 438.8(f)(3)",
                  "props": {
                    "href": "https://www.ecfr.gov/current/title-42/chapter-IV/subchapter-C/part-438/subpart-A/section-438.8#p-438.8(f)(3)",
                    "target": "_blank",
                    "aria-label": "Link opens in new tab"
                  }
                }
              ],
              "mask": "currency",
              "styleAsOptional": true
            }
          },
          {
            "id": "report_mlrDenominator",
            "type": "number",
            "validation": "number",
            "props": {
              "label": "2.3 MLR denominator",
              "hint": [
                {
                  "type": "p",
                  "content": "Enter the required MLR denominator dollar value. If the optional subcomponents are reported in sections 2.1 - 2.2, states must still report the total MLR denominator (i.e., subcomponents will not automatically sum to denominator)."
                },
                {
                  "type": "externalLink",
                  "content": "42 CFR § 438.8(f)(1)",
                  "props": {
                    "href": "https://www.ecfr.gov/current/title-42/chapter-IV/subchapter-C/part-438/subpart-A/section-438.8#p-438.8(f)(1)",
                    "target": "_blank",
                    "aria-label": "Link opens in new tab"
                  }
                }
              ],
              "mask": "currency"
            }
          },
          {
            "id": "report_mlrDenominatorExplanation",
            "type": "textarea",
            "validation": "textOptional",
            "props": {
              "label": "Medical Loss Ratio denominator explanation",
              "hint": "If the optional subcomponents are reported in sections 2.1 and 2.2 and the reported <i>Premium Revenue</i> less the reported <i>Federal, State, and local taxes and licensing and regulatory fees</i> does not equal the total reported <i>MLR denominator</i> in section 2.3, states may provide an explanation here.",
