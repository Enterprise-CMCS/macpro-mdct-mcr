    "@types/prompts": "^2.4.9",
    "@types/node": "*",
    "aws-sdk-client-mock": "^3.1.0",
    "jest": "^28.1.3",
    "prompts": "^2.4.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.2",
