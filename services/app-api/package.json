  "resolutions": {
    "ws": "^8.18.0"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "globals": {
      "isolatedModules": true
    },
    "coverageReporters": [
      "json",
      [
        "lcov",
        {
          "projectRoot": "../../"
        }
      ],
      "text",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "handlers/**/*.{ts,tsx}",
      "utils/**/*.{ts,tsx}",
      "!utils/constants/*",
      "!utils/testing/*",
      "!utils/types/*"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/.build/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/utils/testing/setupJest.ts"
    ]
  }
}